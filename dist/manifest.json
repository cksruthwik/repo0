{
  "manifest_version": 3,
  "name": "Coeus AI Assistant",
  "version": "0.1.0",
  "description": "AI-powered Chatbot, Summarizer, and Mind Mapper.",
  "permissions": [
    "storage",       
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"     
  ],

 
  "icons": {  
    "128": "icons/icon128.png"
  },
  

  "action": {
    "default_popup": "index.html",
    "default_icon": {   
      "128": "icons/icon128.png"  
    }
  }
}